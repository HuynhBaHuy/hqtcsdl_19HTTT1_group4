
    <!--**********************************
        MENU START
    ***********************************-->
    <div class="quixnav">
        <div class="quixnav-scroll">
            <ul class="metismenu" id="menu">
                <li class="nav-label first">Main Menu</li>
                <li><a href="/customer/partner-list" class="menu-item"><i class="far fa-cube"></i></i><span class="nav-text">Danh sách đối tác</span></a></li>
            </ul>
        </div>
    </div>
    <!--**********************************
        MENU END
    ***********************************-->
    
    <!--**********************************
        Content body start
    ***********************************-->
    <div class="content-body">
        <div class="container-fluid">
            <div class="row page-titles mx-0">
                <div class="col-sm-6 p-md-0">
                    <div class="welcome-text">
                        <h4>Hi! Welcome back</h4>
                    </div>
                </div>
            </div>
            <!-- row -->

            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title title-custom">Danh sách đối tác</h4>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered verticle-middle table-responsive-sm partner-table">
                                    <thead>
                                        <tr>
                                            <th class="id-col" scope="col">Mã đối tác</th>
                                            <th class="name-col" scope="col">Tên đối tác</th>
                                            <th class="category-col" scope="col">Mặt hàng kinh doanh</th>
                                            <th class="spokesman-col" scope="col">Người đại diện</th>
                                            <th class="address-col" scope="col">Địa chỉ kinh doanh</th>
                                            <th class="phoneNumber-col" scope="col">Số điện thoại</th>
                                            <th class="email-col" scope="col">Email</th>
                                            <th class="action-col" scope="col"></th>
                                        </tr>
                                    </thead>
                                    <tbody class="table-body">
                                        {{#each partner}}
                                        <tr>
                                            <td>
                                                <div class="id-col">{{id}}</div>
                                            </td>
                                            <td>
                                                <div class="name-col">{{name}}</div>
                                            </td>
                                            <td>
                                                <div class="category-col">{{category}}</div>
                                            </td>
                                            <td>
                                                <div class="spokesman-col">{{spokesman}}</div>
                                            </td>
                                            <td>
                                                <div class="address-col">{{address}}</div>
                                            </td>
                                            <td>
                                                <div class="phoneNumber-col">{{phoneNumber}}</div>
                                            </td>
                                            <td>
                                                <div class="email-col">{{email}}</div>
                                            </td>
                                            <td>
                                                <div id={{id}} class="action-col">
                                                    <button id="showProductList" type="button" class="btn btn-primary">Danh sách sản phẩm</button>
                                                </div>
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!--**********************************
        Content body end
    ***********************************-->


   
</div>
